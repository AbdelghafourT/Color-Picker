{
    "manifest_version": 3,
    "name": "Color Picker - Eyedropper Tool",
    "version": "1.0",
    "description": "Pick colors from anywhere on the web easily with this powerful eyedropper tool.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
        }
    },
    "permissions": [
      "activeTab", 
      "scripting",
      "tabs"
    ]
  }
  